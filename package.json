{
  "name": "proj4",
  "version": "2.10.1-alpha",
  "description": "Proj4js is a JavaScript library to transform point coordinates from one coordinate system to another, including datum transformations.",
  "homepage": "https://proj4js.github.io/proj4js/",
  "main": "dist/proj4-src.js",
  "module": "lib/index.js",
  "directories": {
    "test": "test",
    "doc": "docs"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run rollup && grunt && npm run test:all",
    "build:tmerc": "npm run rollup && grunt build:tmerc  && npm run test:all",
    "rollup": "rollup -c",
    "test": "npm run build && istanbul test node_modules/mocha/bin/_mocha test/test.js",
    "test:all": "npm run test:browser && npm run test:ci",
    "test:browser": "node puppeteer-tests.mjs",
    "test:ci": "istanbul test node_modules/mocha/bin/_mocha test/test-ci.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/proj4js/proj4js.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "chai": "^5.0.0",
    "grunt": "^1.6.1",
    "grunt-cli": "^1.4.3",
    "istanbul": "^0.4.5",
    "mocha": "^10.2.0",
    "rollup": "^4.9.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-eslint": "^9.0.5",
    "tin": "~0.5.0",
    "puppeteer": "^21.7.0"
  },
  "dependencies": {
    "mgrs": "1.0.0",
    "wkt-parser": "^1.3.3"
  }
}
